{
  "prettier.configPath": ".prettierrc.js",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.tslint": true,
    "source.fixAll.stylelint": true
  },
  "eslint.options": {
    "extensions": [".js", ".jsx", ".vue", ".ts", ".tsx"]
  },
  "cSpell.words": [
    "antd",
    "Chaincode",
    "etcdraft",
    "Usci",
    "WENXIANG"
  ],
  "commentTranslate.hover.enabled": false,
}
